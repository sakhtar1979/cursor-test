# MintFlow Mobile App Environment Configuration

# API Configuration
API_URL=http://localhost:8000
ENVIRONMENT=development

# App Configuration
VERSION=1.0.0
BUILD_NUMBER=1

# External Services
SENTRY_DSN=
PLAID_ENV=sandbox

# Feature Flags
ENABLE_BIOMETRIC_AUTH=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_ANALYTICS=true
ENABLE_CRASHLYTICS=true

# Development
DEBUG_MODE=true
LOG_LEVEL=debug

# Security
ENABLE_SSL_PINNING=false